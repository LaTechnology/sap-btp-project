                    <OnDelete Action="Cascade"/>
                </NavigationProperty>
                <NavigationProperty Name="service" Type="incidents.serve.IncidentsService.Appointment">
                    <ReferentialConstraint Property="service_ID" ReferencedProperty="ID"/>
                </NavigationProperty>
                <Property Name="service_ID" Type="Edm.String"/>
            </EntityType>
            <EntityType Name="ServiceWorkers">
