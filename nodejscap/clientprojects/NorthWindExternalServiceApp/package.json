{
  "name": "NorthWindExternalServiceApp", 
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@sap-cloud-sdk/http-client": "^3.9.0",
    "@sap/cds": "^7",
    "express": "^4",
    "passport": "^0",
    "@sap/xssec": "^3"
  },
  "devDependencies": {
    "@cap-js/sqlite": "^1",
    "@sap/cds-dk": "^7"
  },
  "scripts": {
    "start": "cds-serve"
  },
  "cds": {
    "requires": {
      "northwind": {
        "kind": "odata",
        "model": "srv/external/northwind",
        "impl":"",
        "[backend]": {
          "credentials": {
            "url": "https://services.odata.org/Experimental/OData/OData.svc"
          }
        },
        "[production]": {
          "credentials": {
            "destination": "NW_DEV_DESTINATION"
          } 
        }
      },
      "[production]": {
        "auth": {
          "kind": "xsuaa",
          "restrict_all_services": false
        }
      },
      "auth": "xsuaa"
    },
    "features": {
      "kibana_formatter": true
    }
  }
}
