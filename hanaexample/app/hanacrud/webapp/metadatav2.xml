<edmx:DataServices m:DataServiceVersion="2.0">
<Schema xmlns="http://schemas.microsoft.com/ado/2008/09/edm" Namespace="hanaservice">
<EntityContainer Name="EntityContainer" m:sIsDefaultEntityContainer="true">
<EntitySet Name="customer" EntityType="hanaservice.customer"/>
<EntitySet Name="Address" EntityType="hanaservice.Address"/>
