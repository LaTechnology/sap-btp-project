<mvc:View controllerName="ns.hanacrud.controller.Address"
    xmlns:html="http://www.w3.org/1999/xhtml"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m">
  
    <Page >

    <Bar >
<contentMiddle>
<Text text="{customerFirstName} 's Detail Page " textAlign="Center"></Text>
</contentMiddle>

</Bar>

<content>
<VBox >
    
    <List items="{Address}">
    
    <ObjectListItem >
    
    <attributes>
    
    <ObjectAttribute title="Address Line 1"
    
    text="{addressLine1}"
    
    ></ObjectAttribute>
    
    </attributes>
    
    </ObjectListItem>
    
    </List>
    </VBox>
    </content>
    
    
    </Page>


</mvc:View>
