{
  "_version": "1.59.0",
  "sap.app": {
    "id": "library",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.12.2",
      "toolsId": "8e303c34-4613-4054-b62a-522c6f69c375"
    },
    "dataSources": {
      "mainService": {
        "uri": "/v2/odata/v4/my/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "metadata.xml",
          "odataVersion": "2.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.120.4",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.f": {},
        "sap.suite.ui.generic.template": {},
        "sap.ui.comp": {},
        "sap.ui.generic.app": {},
        "sap.ui.table": {},
        "sap.ushell": {}
      }
    },

    "config": {
			"library": {
				"files": [
					"SlideTile.view.xml",
					"Page.controller.js",
					"manifest.json"
				]
			}
		},



    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "library.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "synchronizationMode": "None",
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "library.view",
        "controlAggregation": "pages",
        "controlId": "app",
        "clearControlAggregation": false
      },
      "routes": [
        
        {
          "name": "RouteListview",
          "pattern": ":?query:",
          "target": [
            "TargetListview"
          ]
        }, 
        {
          "name": "Create",
          "pattern": "Create",
          "target": [
            "Create"
          ]
        },
              
        
        {
          "name": "Object",
          "pattern": "Object/{bookshop}",
          "target": [
            "Object"
          ]
        },

        {
          "name": "Edit",
          "pattern": "Edit{id}",
          "target": [
            "Edit"
          ]
        }

      ],
      "targets": {
        "TargetListview": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Listview",
          "viewName": "Listview"
        },
        
        "Create": {
          "viewType": "XML",
          "transition": "flip",
          "clearControlAggregation": false,
          "viewId": "Create",
          "viewName": "Create"
        },

        "Object": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Object",
          "viewName": "Object"
        }
        ,

        "Edit": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Edit",
          "viewName": "Edit"
        }
      }
    },
    "rootView": {
      "viewName": "library.view.App",
      "type": "XML",
      "async": true,
      "id": "App"
    }
  }
}
