<mvc:View xmlns:form="sap.ui.layout.form" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" controllerName="ns.customer.controller.Create" displayBlock="true"><Page>

       <customHeader>


        <Bar xmlns="sap.m">
    <contentLeft>
    <Image src="https://emotions.ae/wp-content/uploads/2019/07/sap-basis-courses-in-dubai.jpg" width="05%" ></Image>
    </contentLeft>

    
    
    </Bar>
    </customHeader>
    

    <Bar >
    
    <contentMiddle>
    
    <Text text="Create a New Entry" wrapping="true" ></Text>
    </contentMiddle>
    
    <contentLeft>
    
    <Button icon="sap-icon://sys-back-2" press="goback" ></Button>
    </contentLeft>
    </Bar>

    
    <form:Form editable="true" id="form0">

       
    
        <form:formContainers>

       
    
            <form:FormContainer id="container1">

       
    
                <form:formElements>

       
    
                    <form:FormElement label="Customer ID" id="element001">

       
    
                        <form:fields>

       
    
                            <Input width="20%" id="customerId"/>

       
    
                        </form:fields>

       
    
                    </form:FormElement>

       
    
                    <form:FormElement label="First Name" id="element002">

       
    
                        <form:fields>

       
    
                            <Input width="20%" id="customerFirstName"/>

       
    
                        </form:fields>

       
    
                    </form:FormElement>

       
    
                    <form:FormElement label="Last Name" id="element003">

       
    
                        <form:fields>

       
    
                            <Input width="20%" id="customerLastName"/>

       
    
                        </form:fields>

       
    
                    </form:FormElement>

       
    
                </form:formElements>

       
    
            </form:FormContainer>

       
    
        </form:formContainers>

       
    
        <form:layout>

       
    
            <form:ResponsiveGridLayout id="layout0"/>

       
    
        </form:layout></form:Form>

 <content>



    </content>

    
       
<footer>
    
    <Bar>
    
    <contentRight>
    
    <Button text="Save" type="Accept" press="savetoDB" icon="sap-icon://save" iconDensityAware="true" width="10%"/>
    <Button text="Cancel" type="Reject" press="cancelProcess" icon="sap-icon://cancel" iconDensityAware="true" width="10%"/>
    
    </contentRight>
    
    </Bar>
    
    </footer>


    

    </Page>

    
    



</mvc:View>