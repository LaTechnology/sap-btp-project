<?xml version="1.0" encoding="utf-8"?>
<edmx:Edmx Version="4.0" xmlns:edmx="http://docs.oasis-open.org/odata/ns/edmx">
    <edmx:Reference Uri="https://sap.github.io/odata-vocabularies/vocabularies/Common.xml">
        <edmx:Include Alias="Common" Namespace="com.sap.vocabularies.Common.v1"/>
    </edmx:Reference>
    <edmx:Reference Uri="https://oasis-tcs.github.io/odata-vocabularies/vocabularies/Org.OData.Core.V1.xml">
        <edmx:Include Alias="Core" Namespace="Org.OData.Core.V1"/>
    </edmx:Reference>
    <edmx:Reference Uri="https://oasis-tcs.github.io/odata-vocabularies/vocabularies/Org.OData.Validation.V1.xml">
        <edmx:Include Alias="Validation" Namespace="Org.OData.Validation.V1"/>
    </edmx:Reference>
    <edmx:DataServices>
        <Schema Namespace="ProcessorService" xmlns="http://docs.oasis-open.org/odata/ns/edm">
            <EntityContainer Name="EntityContainer">
                <EntitySet Name="bookName" EntityType="ProcessorService.bookName">
                    <NavigationPropertyBinding Path="author" Target="authorName"/>
                </EntitySet>
                <EntitySet Name="authorName" EntityType="ProcessorService.authorName">
                    <NavigationPropertyBinding Path="books" Target="bookName"/>
                </EntitySet>
            </EntityContainer>
            <EntityType Name="bookName">
                <Key>
                    <PropertyRef Name="ID"/>
                </Key>
                <Property Name="ID" Type="Edm.String" Nullable="false"/>
                <Property Name="bookName" Type="Edm.String"/>
                <NavigationProperty Name="author" Type="ProcessorService.authorName" Partner="books"/>
                <Property Name="author_ID" Type="Edm.String"/>
                <Property Name="language" Type="Edm.String"/>
                <Property Name="genre" Type="Edm.String"/>
                <Property Name="prize" Type="Edm.Double"/>
            </EntityType>
            <EntityType Name="authorName">
                <Key>
                    <PropertyRef Name="ID"/>
                </Key>
                <Property Name="ID" Type="Edm.String" Nullable="false"/>
                <Property Name="authorName" Type="Edm.String"/>
                <Property Name="publishedYear" Type="Edm.Int32"/>
                <NavigationProperty Name="books" Type="Collection(ProcessorService.bookName)" Partner="author"/>
            </EntityType>
            <Annotations Target="ProcessorService.bookName/genre">
                <Annotation Term="Validation.AllowedValues">
                    <Collection>
                        <Record Type="Validation.AllowedValue">
                            <Annotation Term="Core.SymbolicName" String="Drama"/>
                            <PropertyValue Property="Value" String="Drama"/>
                        </Record>
                        <Record Type="Validation.AllowedValue">
                            <Annotation Term="Core.SymbolicName" String="Mystry"/>
                            <PropertyValue Property="Value" String="Mystry"/>
                        </Record>
                        <Record Type="Validation.AllowedValue">
                            <Annotation Term="Core.SymbolicName" String="Horor"/>
                            <PropertyValue Property="Value" String="Horor"/>
                        </Record>
                    </Collection>
                </Annotation>
            </Annotations>
        </Schema>
    </edmx:DataServices>
</edmx:Edmx>