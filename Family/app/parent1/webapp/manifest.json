{
    "_version": "1.58.0",
    "sap.app": {
        "id": "ns.parent1",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "0.0.1"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "resources": "resources.json",
        "sourceTemplate": {
            "id": "@sap/generator-fiori:lrop",
            "version": "1.11.3",
            "toolsId": "1c707deb-db2d-4719-ae6f-298b25ca112a"
        },
        "dataSources": {
            "mainService": {
                "uri": "/odata/v4/processor/",
                "type": "OData",
                "settings": {
                    "annotations": [],
                    "localUri": "localService/metadata.xml",
                    "odataVersion": "4.0"
                }
            }
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "flexEnabled": true,
        "dependencies": {
            "minUI5Version": "1.120.0",
            "libs": {
                "sap.m": {},
                "sap.ui.core": {},
                "sap.ushell": {},
                "sap.fe.templates": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "ns.parent1.i18n.i18n"
                }
            },
            "": {
                "dataSource": "mainService",
                "preload": true,
                "settings": {
                    "synchronizationMode": "None",
                    "operationMode": "Server",
                    "autoExpandSelect": true,
                    "earlyRequests": true
                }
            },
            "@i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "uri": "i18n/i18n.properties"
            }
        },
        "resources": {
            "css": []
        },
        "routing": {
            "routes": [
                {
                    "pattern": ":?query:",
                    "name": "ParentList",
                    "target": "ParentList"
                },
                {
                    "pattern": "Parent({key}):?query:",
                    "name": "ParentObjectPage",
                    "target": "ParentObjectPage"
                },
                {
                    "pattern": "Parent({key})/child({key2}):?query:",
                    "name": "Parent_childObjectPage",
                    "target": "Parent_childObjectPage"
                }
            ],
            "targets": {
                "ParentList": {
                    "type": "Component",
                    "id": "ParentList",
                    "name": "sap.fe.templates.ListReport",
                    "options": {
                        "settings": {
                            "contextPath": "/Parent",
                            "variantManagement": "Page",
                            "navigation": {
                                "Parent": {
                                    "detail": {
                                        "route": "ParentObjectPage"
                                    }
                                }
                            },
                            "initialLoad": "Enabled",
                            "controlConfiguration": {
                                "@com.sap.vocabularies.UI.v1.LineItem": {
                                    "tableSettings": {
                                        "type": "ResponsiveTable"
                                    }
                                }
                            },
                            "hideFilterBar": false
                        }
                    }
                },
                "ParentObjectPage": {
                    "type": "Component",
                    "id": "ParentObjectPage",
                    "name": "sap.fe.templates.ObjectPage",
                    "options": {
                        "settings": {
                            "editableHeaderContent": true,
                            "contextPath": "/Parent",
                            "showRelatedApps": true,
                            "content": {
                                "header": {
                                    "visible": true
                                }
                            },
                            "sectionLayout": "Tabs",
                            "navigation": {
                                "child": {
                                    "detail": {
                                        "route": "Parent_childObjectPage"
                                    }
                                }
                            },
                            "controlConfiguration": {
                                "child/@com.sap.vocabularies.UI.v1.LineItem#ChildSection": {
                                    "tableSettings": {
                                        "enableFullScreen": true,
                                        "enablePaste": true,
                                        "type": "ResponsiveTable",
                                        "creationMode": {
                                            "createAtEnd": true
                                        },
                                        "personalization": {
                                            "column": true,
                                            "filter": true
                                        }
                                    }
                                }
                            }
                        }
                    }
                },
                "Parent_childObjectPage": {
                    "type": "Component",
                    "id": "Parent_childObjectPage",
                    "name": "sap.fe.templates.ObjectPage",
                    "options": {
                        "settings": {
                            "contextPath": "/Parent/child",
                            "navigation": {},
                            "controlConfiguration": {
                                "grandchild/@com.sap.vocabularies.UI.v1.LineItem#sampleTable": {
                                    "columns": {
                                        "ID": {
                                            "header": "Child Table",
                                            "position": {
                                                "placement": "After"
                                            },
                                            "template": "ns.parent1.ext.fragment.ID"
                                        },
                                        "Name": {
                                            "header": "Child Table 2",
                                            "position": {
                                                "placement": "After"
                                            },
                                            "template": "ns.parent1.ext.fragment.Name"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "extends": {
            "extensions": {
                "sap.ui.controllerExtensions": {}
            }
        }
    },
    "sap.fiori": {
        "registrationIds": [],
        "archeType": "transactional"
    }
}
