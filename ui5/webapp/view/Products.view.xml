<mvc:View 
xmlns:table="sap.ui.table"
controllerName="ui5.walkthrough.controller.products"
xmlns="sap.m"
xmlns:mvc="sap.ui.core.mvc"
displayBlock="true">

<App >

<pages>

<Page title="Products" titleAlignment="Center" >

<content>

 <List
         headerText="{i18n>productsListTitle}"
         
         class="sapUiResponsiveMargin"
         width="auto"
         items="{products>/Products}" 
      >
      <items>
      
         <ObjectListItem

            title="{products>Title}"

            icon="{products>image}"
            
            
            type="Navigation"
            press=".onPress"/>
      </items>
   </List>




</content>

</Page>

</pages>

</App>

 
</mvc:View>         