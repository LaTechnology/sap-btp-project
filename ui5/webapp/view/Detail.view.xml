<mvc:View xmlns:core="sap.ui.core" xmlns:form="sap.ui.layout.form" xmlns:tnt="sap.tnt" xmlns:launchpad="sap.ushell.ui.launchpad" xmlns:uxap="sap.uxap" xmlns:table="sap.ui.table"
	controllerName="ui5.walkthrough.controller.Detail"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc">



<App>
<pages>
	
	<Page
		title="{i18n>detailPageTitle}" showNavButton="true" navButtonPress=".onNavBack">

		<Bar >
		<contentMiddle>
		
		<Title text="{products>Title}'s Detail "></Title>
		
		</contentMiddle>
		
		</Bar>

			

			<VBox class="sapUiSmallMarginTopBottom sapUiLargeMarginBeginEnd">
		<HBox justifyContent="SpaceBetween">
			<VBox>
				
				<Image src="{products>image}" width="20%" visible="true"  />

				
			</VBox>
			</HBox>
			</VBox>

		

		<content>

	

		

		<ObjectHeader


	
	>




	

	<attributes>

	

	
					
						

						<ObjectAttribute
						title="{i18n>FirstName}"
						text="{products>FirstName}"/>

						<ObjectAttribute
						title="{i18n>LasttName}"
						text="{products>LastName}"/>

						<ObjectAttribute
						title="{i18n>Address}"
						text="{products>Address}"/>

						<ObjectAttribute
						title="{i18n>City}"
						text="{products>City}"/>

							<ObjectAttribute
						title="{i18n>PostalCode}"
						text="{products>PostalCode}"/>

						<ObjectAttribute
						title="{i18n>Country}"
						text="{products>Country}"/>

						<ObjectAttribute
						title="{i18n>HomePhone}"
						text="{products>HomePhone}"/>

					

						</attributes>

						
					

</ObjectHeader>



	</content>
	</Page>

</pages>
	</App>

</mvc:View>