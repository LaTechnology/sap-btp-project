VIEW ProcessorService_Author AS SELECT
  Author_0.createdAt,
  Author_0.createdBy,
  Author_0.modifiedAt,
  Author_0.modifiedBy,
  Author_0.ID,
  Author_0.name
FROM sap_capire_incidents_Author AS Author_0
WITH ASSOCIATIONS (
  MANY TO MANY JOIN ProcessorService_Book AS books ON (books.author_ID = books.ID)
)