<mvc:View
    xmlns:layout="sap.ui.layout"
    controllerName="ns.propose.controller.Main"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
    xmlns="sap.m"
>
    <Shell id="_IDGenShell1">
        <Page
            id="page"
            title="{i18n>title}"
        >
            <customHeader>
                <OverflowToolbar id="_IDGenOverflowToolbar1">
                    <Bar id="_IDGenBar1">

                        <contentMiddle>
                            <Text id="_IDGenText1" text="{i18n>headerTitle}" />
                        </contentMiddle>
                    </Bar>
                </OverflowToolbar>
            </customHeader>
            <content>
                <Bar id="_IDGenBar2" class="mainpageBar" />

               
                    <GenericTile id="_IDGenGenericTile1"
                        header="vendor Creation"
                        width="15%"
                        press=".onNavVendor"
                    >
                        <!-- <tileContent>
                            <TileContent id="_IDGenTileContent1">
                                <Text id="_IDGenText2" text="create new details" />
                            </TileContent>
                        </tileContent> -->
                    </GenericTile>
               <GenericTile press=".onNavClient" id="_IDGenGenericTile2" header="client Creation"></GenericTile>
                <GenericTile press=".onNavSupplierView" id="_IDGenGenericTile4" header="Supllier view"></GenericTile>
               <GenericTile press=".onNavClientView" id="_IDGenGenericTile5" header="Client view"></GenericTile>
               <GenericTile press=".onNavTemplate" id="_IDGenGenericTile3" header="Template Creation"></GenericTile>
            </content>
        </Page>
    </Shell>
</mvc:View>
