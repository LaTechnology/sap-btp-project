<mvc:View xmlns:unified="sap.ui.unified"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m"
    xmlns:f="sap.ui.layout.form"
    controllerName="ns.propose.controller.SupplierView">
    <Page id="_IDGenPage1">
        <f:SimpleForm
            id="clientForm"
            title="Preview"
            layout="ResponsiveGridLayout"
            labelSpanXL="3"
            labelSpanL="3"
            labelSpanM="3"
            labelSpanS="12"
            adjustLabelSpan="false">
            <f:content>
                <Label text="Client ID:" class="blueLabel"/>
                <Text text="{id}" class="boldText"/>
                <Label text="Client Name:" class="blueLabel"/>
                <Text text="{name}" class="boldText"/>
                <Label text="Client Website:" class="blueLabel"/>
                <Text text="{website}" class="boldText"/>
                <!-- <Text text="{path: 'PS_CUSTOMER_ORG_CONTACT', parameters: {expand: 'PS_CUSTOMER_ORG'}}"/> -->
            </f:content>
        </f:SimpleForm>
        
    </Page>
</mvc:View>
