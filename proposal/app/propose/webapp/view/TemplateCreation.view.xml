    xmlns:html="http://www.w3.org/1999/xhtml"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
    xmlns="sap.m"
    xmlns:wizard="sap.m.Wizard"
>
    <NavContainer id="_IDGenNavContainer1">
        <pages>
            <Page
                id="_IDGenPage1"
                title="Proposal Client Form"
                class="prorposal"
            >
                <customHeader>
                    <Bar id="_IDGenBar1">
                        <contentMiddle>
                            <Title id="_IDGenTitle1" text="Proposal Client Form" />
                        </contentMiddle>
                    </Bar>
                </customHeader>

                <Bar id="_IDGenBar2">
                    <contentLeft>
                        <Image id="_IDGenImage1"
                            src="{i18n>headerimage}"
                            width="50%"
                        />
                    </contentLeft>

                    <contentRight>
                        <Image id="_IDGenImage2"
                            src="https://tier4marketing.com/wp-content/uploads/2015/10/Client-Logo-Placeholder.png"
                            width="40%"
                        />
                    </contentRight>
                </Bar>

                <Wizard
                    id="wizard"
                    stepActivate="onStepChanged"
                    showNextButton="true"
                    class="proposalform"
                >
                    <steps>
                        <WizardStep
                            id="wizard1"
                            title="Step 1"
                            nextStep="nextstep1"
                            complete="true"
                            class="proposalform"
                        >
                            <MessageStrip id="_IDGenMessageStrip1"
                                class="sapUiSmallMarginBottom"
                                text="Enter the required details"
                                showIcon="true"
                            />
                            <VBox id="_IDGenVBox1">
                                <Table
                                    id="idFrame0"
                                    growing="true"
                                    growingDirection="Downwards"
                                    class="table"
                                >
                                    <headerToolbar>
                                        <Toolbar id="_IDGenToolbar1">
                                            <ToolbarSpacer
                                                id="_IDGenToolbarSpacer1"
                                            />
                                            <Button
                                                id="_IDGenButton1"
                                                press="onAddRowTextArea"
                                                text="Add Description"
                                                class="buttonfortool"
                                                icon="sap-icon://add-coursebook"
                                                iconDensityAware="true"
                                            />

                                            <Button id="_IDGenButton9"
                                                text="Add Sub-Section"
                                                class="buttonfortool"
                                                press="onSubSection"
                                                icon="sap-icon://sys-add"
                                            />

                                            <Button id="_IDGenButton10"
                                                    
                                                    text="Add Image"
                                                    press="onAddImage"
                                                    class="buttonfortool"
                                                    icon="sap-icon://picture"/>
                                        </Toolbar>
                                    </headerToolbar>
                                    <columns>
                                        <Column id="coloumn1" />
                                    </columns>

                                    <items>
                                        <ColumnListItem
                                            id="_IDGenColumnListItem1"
                                        >
                                            <cells />
                                        </ColumnListItem>
                                    </items>
                                </Table>
                            </VBox>
                        </WizardStep>

                        <WizardStep
                            title="Step 2"
                            id="wizard2"
                            class="proposalform"
                        >
