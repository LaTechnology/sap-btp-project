                <footer>
                    <Bar id="_IDGenBar3">
                        <contentRight>
                            <Button id="_IDGenButton18"
                                text="Next"
                                press="onContinue"
                                type="Emphasized"
                            />
                            <Button id="_IDGenButton19"
                                text="Save"
