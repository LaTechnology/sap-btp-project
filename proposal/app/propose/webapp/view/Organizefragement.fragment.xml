<core:FragmentDefinition xmlns:cards="sap.f.cards"
    xmlns:uxap="sap.uxap"
   controllerName="ns.propose.controller.Main"
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
    xmlns:f="sap.ui.layout.form"
    displayBlock="true"
>
    <Dialog id="AddcarDialog" title="Org Creation">
        <VBox id="_IDGenVBox1" justifyContent="Center" alignItems="Center" >

            <Button id="_IDGenButton1"
                text="Vendor"
                press=".cvButton"
                class="buttonCenter"
            />

            <Button id="_IDGenButton2"
                text="Client"
                press=".cvButtonc"
                class="buttonCenter"
            />
        </VBox>
        <VBox id="_IDGenVBox2" justifyContent="Center" alignItems="Center">

            <Button id="_IDGenButton3" icon="sap-icon://decline" press=".onClose"></Button>

        </VBox>
    </Dialog>
</core:FragmentDefinition>
