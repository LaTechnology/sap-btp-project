<mvc:View xmlns:f="sap.f" xmlns:macros="sap.fe.macros" xmlns:form="sap.ui.layout.form" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" controllerName="ns.demoapp.controller.App" displayBlock="true">

<App id="app002">

    <pages>
        <Page id="page002" backgroundDesign="Transparent" title="Object Page">

            <customHeader>

            

            <Bar id="bar002" >

                <contentMiddle>
                    <Title id="title002"
                    text="Employee Details" level="H1"
                    textAlign="Initial"
                    textDirection="Inherit"
                    titleStyle="H5"></Title>
                </contentMiddle>


            </Bar>
        </customHeader>

      <List id="listobj001" growing="true" headerText="Employee Full Detail" items="{/Employees}">

        <items>
            <StandardListItem 
            
            title="{FirstName} {LastName}"
            type="Navigation"
            press="namingpress"

            ></StandardListItem>
        </items>

      </List>
        </Page>
    </pages>
</App>
</mvc:View>