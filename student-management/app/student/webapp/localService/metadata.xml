<?xml version="1.0" encoding="utf-8"?>
<edmx:Edmx Version="4.0" xmlns:edmx="http://docs.oasis-open.org/odata/ns/edmx">
    <edmx:Reference Uri="https://sap.github.io/odata-vocabularies/vocabularies/Common.xml">
        <edmx:Include Alias="Common" Namespace="com.sap.vocabularies.Common.v1"/>
    </edmx:Reference>
    <edmx:Reference Uri="https://oasis-tcs.github.io/odata-vocabularies/vocabularies/Org.OData.Core.V1.xml">
        <edmx:Include Alias="Core" Namespace="Org.OData.Core.V1"/>
    </edmx:Reference>
    <edmx:DataServices>
        <Schema Namespace="StudentManagement" xmlns="http://docs.oasis-open.org/odata/ns/edm">
            <EntityContainer Name="EntityContainer">
                <EntitySet Name="StudentDetail" EntityType="StudentManagement.StudentDetail">
                    <NavigationPropertyBinding Path="contact" Target="ContactDetails"/>
                    <NavigationPropertyBinding Path="addressTypes" Target="addressDetail"/>
                </EntitySet>
                <EntitySet Name="addressDetail" EntityType="StudentManagement.addressDetail">
                    <NavigationPropertyBinding Path="address" Target="StudentDetail"/>
                </EntitySet>
                <EntitySet Name="ContactDetails" EntityType="StudentManagement.ContactDetails"/>
            </EntityContainer>
            <EntityType Name="StudentDetail">
                <Key>
                    <PropertyRef Name="studentId"/>
                </Key>
                <Property Name="studentId" Type="Edm.String" Nullable="false"/>
                <Property Name="name" Type="Edm.String"/>
                <NavigationProperty Name="contact" Type="StudentManagement.ContactDetails">
                    <ReferentialConstraint Property="contact_id" ReferencedProperty="id"/>
                </NavigationProperty>
                <Property Name="contact_id" Type="Edm.String"/>
                <NavigationProperty Name="addressTypes" Type="Collection(StudentManagement.addressDetail)" Partner="address"/>
                <Property Name="cityName" Type="Edm.String" MaxLength="10"/>
            </EntityType>
            <EntityType Name="addressDetail">
                <Key>
                    <PropertyRef Name="id"/>
                </Key>
                <Property Name="id" Type="Edm.String" Nullable="false"/>
                <Property Name="addressLine1" Type="Edm.String"/>
                <Property Name="addressLine2" Type="Edm.String"/>
                <Property Name="city" Type="Edm.String"/>
                <Property Name="pincode" Type="Edm.String" MaxLength="10"/>
                <NavigationProperty Name="address" Type="StudentManagement.StudentDetail" Partner="addressTypes">
                    <ReferentialConstraint Property="address_studentId" ReferencedProperty="studentId"/>
                </NavigationProperty>
                <Property Name="address_studentId" Type="Edm.String"/>
            </EntityType>
            <EntityType Name="ContactDetails">
                <Key>
                    <PropertyRef Name="id"/>
                </Key>
                <Property Name="id" Type="Edm.String" Nullable="false"/>
                <Property Name="number" Type="Edm.String" MaxLength="13"/>
            </EntityType>
        </Schema>
    </edmx:DataServices>
</edmx:Edmx>