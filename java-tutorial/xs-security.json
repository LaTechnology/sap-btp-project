{
    "xsappname": "helloworld",
    "tenant-mode": "dedicated",
      "oauth2-configuration": {
    "redirect-uris": [
        "https://*.cfapps.eu20.hana.ondemand.com/**"
      ]
    },
    "scopes": [
      {
        "name": "$XSAPPNAME.Display",
        "description": "Display content"
      }
    ],
    "role-templates": [
      {
        "name": "Viewer",
        "description": "View content",
        "scope-references": [
          "$XSAPPNAME.Display"
        ]
      }
    ]
  }
