<core:FragmentDefinition
   xmlns="sap.m"
   xmlns:core="sap.ui.core">
   <Dialog
      id="helloDialog"
      title="Hello {/recipient/name}"
      draggable="true"
      icon="data:image/jpeg;base64,"
      visible="true"
      afterClose="kick me"
      busy="false"
      contentHeight="10px"
      resizable="true"
      showHeader="true"
      horizontalScrolling="true"
      stretch="true"
      titleAlignment="Center"
      contentWidth="100%"
      >
      <content>
         <core:Icon
            src="sap-icon://hello-world"
            size="8rem"
            class="sapUiMediumMargin"/>
      </content>
        <beginButton>
            <Button text="{i18n>DialogCloseButtonText}"
            press=".onCloseDialog"
            icon="sap-icon://world"
            textDirection="RTL"
            busy="false"
            ></Button>
        </beginButton>
      </Dialog>
</core:FragmentDefinition>