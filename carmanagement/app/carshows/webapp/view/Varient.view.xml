<mvc:View
    controllerName="ns.carshows.controller.Varient"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc" displayBlock="true">
	<Table id="_IDGenTable1" items="{path : 'modelVarient'}" growing="true"
growingThreshold="5">
              <headerToolbar >
         <Toolbar>
            <Title text="{i18n>carAssociation}" />
            <ToolbarSpacer/>
            <SearchField 
               width="50%" 
               search="onFilterCar"/>
         </Toolbar>
      </headerToolbar>
                <columns>
				<Column id="_IDGenColumn5">
                    <Label id="_IDGenLabel5" text="ID"></Label>
                    </Column>
                       <Column id="_IDGenColumn4">
                    <Label id="_IDGenLabel4" text="VARIENTS"></Label>
                    </Column>
					
                   </columns>
                   <items>
                <ColumnListItem
                 id="_IDGenColumnListItem1" type="Navigation" press=".onListItemPress">
                        <cells>
						    <Text id="_IDGenText5" text="{Id}"></Text>
                            <Text id="_IDGenText4" text="{modelName}"></Text>
                        </cells>                    
                    </ColumnListItem>
                </items>
            </Table>
			
</mvc:View>