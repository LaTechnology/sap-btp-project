  "scripts": {
    "start": "cds-serve",
    "watch-bpartner": "cds watch --open bpartner/webapp/index.html?sap-ui-xx-viewCache=false",
    "watch-bpaddress": "cds watch --open bpaddress/webapp/index.html?sap-ui-xx-viewCache=false",
    "watch-bpemail": "cds watch --open bpemail/webapp/index.html?sap-ui-xx-viewCache=false",
    "watch-addressfax": "cds watch --open addressfax/webapp/index.html?sap-ui-xx-viewCache=false",
