  "cds": {
    "requires": {
      "API_BUSINESS_PARTNER": {
        "kind": "odata-v2",
        "model": "srv/external/API_BUSINESS_PARTNER"
      },
      "db": "hana"
    }
  },
  "sapux": [
    "app/bpartner",
    "app/bpaddress",
    "app/bpemail",
    "app/addressfax",
    "app/homeurl",
    "app/phonenumber",
    "app/deplocalnumber",
    "app/contacttoaddress",
    "app/contacttoaddress",
    "app/contactfunctionanddept",
    "app/bpbank"
