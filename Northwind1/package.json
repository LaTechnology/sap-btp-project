{
  "name": "com.ladera.northwind",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": {
    "type": "git",
    "url": "<Add your repository here>"
  },
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@sap-cloud-sdk/http-client": "^3.10.0",
    "@sap/cds": "^7",
    "express": "^4.18.2",
    "node-cron": "^3.0.3"
  },
  "devDependencies": {
    "@cap-js/sqlite": "^1"
  },
  "scripts": {
    "start": "cds-serve"
  },
  "cds": {
    "requires": {
      "Northwind": {
        "kind": "odata",
        "model": "srv/external/NorthWind",
        "[backend]": {
          "credentials": {
            "url": "https://services.odata.org/Experimental/OData/OData.svc"
          }
        }
      },
      "db": {
        "kind": "sqlite",
        "credentials": {
          "url": "db.sqlite"
        }
      }
    }
  },
  "main": "app.js",
  "keywords": [
    "Northwind"
  ],
  "author": "ladera"
}
