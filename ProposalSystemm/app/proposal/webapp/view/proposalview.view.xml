<mvc:View
    xmlns:layout="sap.ui.layout"
    controllerName="ns.proposal.controller.proposalview"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
    xmlns="sap.m"
>
    <Shell id="_IDGenShell1">
        <Page 
            id="page"
            title="{i18n>title}"
        >
            <customHeader>
                <OverflowToolbar id="_IDGenOverflowToolbar1">
                    <Bar id="_IDGenBar1">
                        <contentLeft>
                            <Image id="_IDGenImage1"
                                src="{i18n>headerImage}"
                                width="100%"
                                height="400%"
                            />
                        </contentLeft>

                        <contentMiddle>
                            <Text id="_IDGenText1" text="{i18n>title}" />
                        </contentMiddle>
                    </Bar>
                </OverflowToolbar>
            </customHeader>
            <content>
                <Bar id="_IDGenBar2" class="mainpageBar" />

                    <GenericTile id="_IDGenGenericTile1"
                    class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout sapUiResponsiveMargin"
                        header="Supplier Creation"
                        width="15%"
                        press=".onNavOrganization"
                        
                    >
                        <tileContent>
                            <TileContent id="_IDGenTileContent1">
                                <Text id="_IDGenText2" text="Create Supplier" />
                            </TileContent>
                        </tileContent>
                    </GenericTile>

                    <GenericTile id="_IDGenGenericTile2"
                    class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout sapUiResponsiveMargin"
                        header="Customer Creation"
                        width="15%"
                        press=".onNavClientOrganization"
                    
                    >
                        <tileContent>
                            <TileContent id="_IDGenTileContent2">
                                <Text id="_IDGenText3" text="Create Customer" />
                            </TileContent>
                        </tileContent>
                    </GenericTile>
                
               
               <!-- <GenericTile 
               header="Template Creation"
               ></GenericTile> -->
            </content>
        </Page>
    </Shell>
</mvc:View>