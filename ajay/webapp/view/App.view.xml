<mvc:View xmlns:form="sap.ui.layout.form" xmlns:launchpad="sap.ushell.ui.launchpad" 

xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" 

xmlns:l="sap.ui.layout" controllerName="sap.com.ajay.controller.App" displayBlock="false">
    

<Page xmlns="sap.m" id="pageId">
    
    <customHeader>

        <Bar>

            <contentMiddle>
                
                <Image src="https://ui5.sap.com/resources/sap/ui/documentation/sdk/images/logo_ui5.png" width="140px" height="140px" id="image0"/>
                
                <Title level="H1" text="Welcome to UI5Demo" textAlign="Center"/>
            </contentMiddle>

            <contentLeft>
                <Button icon="sap-icon://dishwasher" press="clickme" text="Click me"/>
            </contentLeft>

            <contentRight>
                <Button xmlns="sap.m" text="Login" id="Login001" icon="sap-icon://add-employee" press="ajaylogin"/>
            </contentRight>

        </Bar>

    </customHeader>

   


  
       
  
  
    <content>
  
        <sap.ui.layout.form:Form xmlns:sap.ui.layout.form="sap.ui.layout.form" editable="true" id="form0">
  
            <sap.ui.layout.form:formContainers>
  
                <sap.ui.layout.form:FormContainer title="Signup" id="container2">
  
                    <sap.ui.layout.form:formElements><sap.ui.layout.form:FormElement label="First Name" id="Name1">
  
                            <sap.ui.layout.form:fields>
  
                                <Input width="45%" id="input3" placeholder="Enter your First Name"/>
  
                            </sap.ui.layout.form:fields>
  
                        </sap.ui.layout.form:FormElement>
  
                        <sap.ui.layout.form:FormElement label="Last Name" id="Name2">
  
                            <sap.ui.layout.form:fields>
  
                                <Input width="45%" id="input8" placeholder="Enter your Last Name"/>
  
                            </sap.ui.layout.form:fields>
  
                        </sap.ui.layout.form:FormElement>
  
                        <sap.ui.layout.form:FormElement label="DOB" id="DOB">
  
                            <sap.ui.layout.form:fields>
  
                                <DatePicker xmlns="sap.m" id="picker0" secondaryCalendarType="Persian" width="45%"/>
  
                            </sap.ui.layout.form:fields>
  
                        </sap.ui.layout.form:FormElement>
  
                        <sap.ui.layout.form:FormElement label="Email Address" id="Email">
  
                            <sap.ui.layout.form:fields>
  
                                <Input width="45%" id="input6" placeholder="Enter your Email Address"/>
  
                            </sap.ui.layout.form:fields>
  
                        </sap.ui.layout.form:FormElement>
  
                    </sap.ui.layout.form:formElements>
  
                </sap.ui.layout.form:FormContainer>
  
            </sap.ui.layout.form:formContainers>
  
            <sap.ui.layout.form:layout>
  
                <sap.ui.layout.form:ResponsiveGridLayout id="layout0"/>
  
            </sap.ui.layout.form:layout></sap.ui.layout.form:Form>
  
        <Button xmlns="sap.m" text="Button" id="button0"/>
  
    </content>

    
  
    </Page>

  
</mvc:View>