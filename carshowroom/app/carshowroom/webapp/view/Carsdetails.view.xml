<mvc:View
    xmlns:form="sap.ui.layout.form"
    xmlns:cards="sap.f.cards"
    xmlns:table="sap.ui.table"
    controllerName="my.carshowroom.controller.Carsdetails"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
    xmlns="sap.m"

>

<Bar >
        <contentLeft>
            <Title
                text="Car Details"
                level="H1"
                titleStyle="H3"
            />
        </contentLeft>
    </Bar>
    <VBox
        alignItems="Center"
        justifyContent="Start"
    >
        <Image
            src="{carImage}"
            alt="Car Lable"
            width="100px"
            height="100px"
        />
    </VBox>
    <List>
        <ObjectListItem>
            <attributes>
                <ObjectAttribute
                    class="bigFont"
                    title="Car Name"
                    text="{carName}"
                />

                <ObjectAttribute
                    class="bigFont"
                    title="Car Model"
                    text="{carModel}"
                />

                <ObjectAttribute
                    class="bigFont"
                    title="Engine Type"
                    text="{engineType}"
                />

                <ObjectAttribute
                    class="bigFont"
                    title="Model Year"
                    text="{modelYear}"
                />
            </attributes>
        </ObjectListItem>
    </List>

    <Bar >
        <contentLeft>
            <Title
                text="Model Details"
                level="H1"
                titleStyle="H3"
            />
        </contentLeft>
    </Bar>

   

    <!-- <content> -->
    <!-- <mvc:XMLView viewData="my.carshowroom.controller.Modeldetails"></mvc:XMLView> -->
    <!-- </content> -->
</mvc:View>
