<mvc:View
    controllerName="my.carshowroom.controller.Carsdetails"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
    xmlns="sap.m"
>
    <Page title="{i18n>title}">
        <content>

         <ObjectHeader
                responsive="true"
                title="{carName}"
                intro="Welcome To {carName} Showroom"
                icon="sap-icon://car-rental"
            />
            <mvc:XMLView viewName="my.carshowroom.view.Carsdetails"/>
            <mvc:XMLView viewName="my.carshowroom.view.Varient"/>
        </content>
    </Page>
</mvc:View>
