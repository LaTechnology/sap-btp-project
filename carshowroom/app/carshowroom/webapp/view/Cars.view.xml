<mvc:View
    controllerName="my.carshowroom.controller.Cars"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
    xmlns="sap.m"
>
    <Page
        id="page"
        title="{i18n>title}"
    >
        <content>
            <Table
                id="_IDGenTable1"
                items="{/cars}"
            >
                <headerToolbar>
                    <Toolbar id="_IDGenToolbar1">
                        <Title
                            id="_IDGenTitle1"
                            text="{i18n>carListTitle}"
                        />
                        <ToolbarSpacer id="_IDGenToolbarSpacer1" />
                        <SearchField
                            id="_IDGenSearchField1"
                            width="50%"
                            search=".onFilterCar"
                        />
                    </Toolbar>
                </headerToolbar>
                <columns>
                    <Column id="_IDGenColumn1">
                        <Label
                            id="_IDGenLabel1"
                            text="List"
                        />
                    </Column>
                    <Column id="_IDGenColumn2">
                        <Label
                            id="_IDGenLabel2"
                            text="Car Name"
                        />
                    </Column>
                    <Column id="_IDGenColumn3">
                        <Label
                            id="_IDGenLabel3"
                            text="Car Model"
                        />
                    </Column>
                    <Column id="_IDGenColumn4">
                        <Label
                            id="_IDGenLabel4"
                            text="Engine Type"
                        />
                    </Column>
                    <Column id="_IDGenColumn5">
                        <Label
                            id="_IDGenLabel5"
                            text="Model Year"
                        />
                    </Column>
                </columns>
                <items>
                    <ColumnListItem
                        id="_IDGenColumnListItem1"
                        press="onPress"
                        type="Navigation"
                    >
                        <cells>
                            <Text
                                id="_IDGenText1"
                                text="{ID}"
                            />
                            <Text
                                id="_IDGenText2"
                                text="{carName}"
                            />
                            <Text
                                id="_IDGenText3"
                                text="{carModel}"
                            />
                            <Text
                                id="_IDGenText4"
                                text="{engineType}"
                            />
                            <Text
                                id="_IDGenText5"
                                text="{modelYear}"
                            />
                        </cells>
                    </ColumnListItem>
                </items>
            </Table>
        </content>
    </Page>
</mvc:View>
