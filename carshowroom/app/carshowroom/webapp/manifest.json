{
  "_version": "1.59.0",
  "sap.app": {
    "id": "my.carshowroom",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.11.5",
      "toolsId": "28aaf271-6616-42a8-a3be-6114e51dc3de"
    },
    "dataSources": {
      "mainService": {
        "uri": "/v2/odata/v4/car-showroom/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/metadata.xml",
          "odataVersion": "2.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",


    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    },
    
    "icons": {
      "icon": "sap-icon://Fiori2/Fiori2",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""

    },

    
    "fullWidth": true

  },


  "sap.ui5": {
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.120.4",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.f": {},
        "sap.suite.ui.generic.template": {},
        "sap.ui.comp": {},
        "sap.ui.generic.app": {},
        "sap.ui.table": {},
        "sap.ushell": {},
        "sap.ui.layout": {},
        "sap.uxap": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "my.carshowroom.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "synchronizationMode": "None",
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      }
    },

    "resourceBundle": "i18n/i18n.properties",
    "supportedThemes": ["sap_belize_plus", "sap_fiori_3_dark"],
    
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "my.carshowroom.view",
        "controlAggregation": "pages",
        "controlId": "app",
        "clearControlAggregation": false
      },
      "routes": [
        {
          "name": "RouteDetails",
          "pattern": "",
          "target": ["TargetCars"]
        },
        {
          "name": "RouteCars",
          "pattern": "Carsdetails/{invoicePath}",
          "target": ["TargetCarsdetails"]
        }
      ],
      "targets": {
        "TargetCars": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Cars",
          "viewName": "Cars"
        },
        "TargetCarsdetails": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Modeldetails",
          "viewName": "Modeldetails"
        }
      }
    },

    "rootView": {
      "viewName": "my.carshowroom.view.App",
      "type": "XML",
      "async": true,
      "id": "App"
    }
  }
}
