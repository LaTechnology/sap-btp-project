{
  "name": "NorthWindExternalService",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@sap-cloud-sdk/http-client": "^3.9.0",
    "@sap-cloud-sdk/resilience": "^3.9.0",
    "@sap/cds": "^7",
    "@sap/xssec": "^3",
    "express": "^4",
    "passport": "^0"
  },
  "devDependencies": {
    "@cap-js/sqlite": "^1",
    "@sap/cds-dk": "^7"
  },
  "scripts": {
    "start": "cds-serve"
  },
  "cds": {
    "requires": {
      "northwindexternalservice": {
        "kind": "odata",
        "model": "srv/external/northwindexternalservice",
        "[backend]": {
          "credentials": {
            "url": "https://services.odata.org/Experimental/OData/OData.svc/"
          }
        },
        "[production]": {
          "credentials": {
            "destination": "NORTHWIND_DEVOLPMENT_DESTINATION"
          }
        }
      },
      "[production]": {
        "auth": {
          "kind": "xsuaa",
          "restrict_all_services": false
        }
      }
    }
  }
}
