<mvc:View xmlns:table="sap.ui.table" xmlns:smartfilterbar="sap.ui.comp.smartfilterbar" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" controllerName="ns.odataapi.controller.employeelist" 

displayBlock="true"

xmlns:smarttable="sap.ui.comp.smarttable"

>

<Page >

<content>


<Table items="{/A_BusinessPartner}">

<columns>
                    <Column width="10%" > 
                        <Label text="Customer Id"></Label>
                    </Column>
                     <Column width="20%">
                        <Label text="Customer Name"></Label>
                    </Column>
                    <Column width="30%">
                    <Label text="ETag"></Label>
                    </Column>
                    <Column width="5%">
                   
                    </Column>
                   </columns>
                   <items>
                
                <ColumnListItem press="nextpage" type="Navigation">
                        <cells>
                            <Text text="{BusinessPartner}" ></Text>
                            <Text text="{BusinessPartnerName}"></Text>
                            <Text text="{ETag}"></Text>
                            <Button icon="sap-icon://open-command-field" id="button001" press=".nextpage"></Button>
                         
                        </cells>                    
                    </ColumnListItem>
                </items>


</Table>



</content>


</Page>

</mvc:View>