<mvc:View
   xmlns="sap.m"
   xmlns:mvc="sap.ui.core.mvc">
   <List
      headerText="{i18n>Employee Names}"
      class="sapUiResponsiveMargin"
      width="auto"
      items="{employee>/Employee}" >
      <items>
         <ObjectListItem
            title="{employee>FirstName} {employee>LastName}"/>
      </items>
   </List>
</mvc:View>