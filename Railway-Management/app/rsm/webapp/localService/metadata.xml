<?xml version="1.0" encoding="utf-8"?>
<edmx:Edmx Version="4.0" xmlns:edmx="http://docs.oasis-open.org/odata/ns/edmx">
    <edmx:Reference Uri="https://sap.github.io/odata-vocabularies/vocabularies/Common.xml">
        <edmx:Include Alias="Common" Namespace="com.sap.vocabularies.Common.v1"/>
    </edmx:Reference>
    <edmx:Reference Uri="https://oasis-tcs.github.io/odata-vocabularies/vocabularies/Org.OData.Core.V1.xml">
        <edmx:Include Alias="Core" Namespace="Org.OData.Core.V1"/>
    </edmx:Reference>
    <edmx:DataServices>
        <Schema Namespace="Railway" xmlns="http://docs.oasis-open.org/odata/ns/edm">
            <EntityContainer Name="EntityContainer">
                <EntitySet Name="Employes_Detail" EntityType="Railway.Employes_Detail">
                    <NavigationPropertyBinding Path="department_det" Target="department"/>
                    <NavigationPropertyBinding Path="address" Target="Employes_Detail_address"/>
                </EntitySet>
                <EntitySet Name="department" EntityType="Railway.department">
                    <NavigationPropertyBinding Path="Emp_Id_" Target="Employes_Detail"/>
                </EntitySet>
                <EntitySet Name="organisation" EntityType="Railway.organisation">
                    <NavigationPropertyBinding Path="employees_details" Target="Employes_Detail"/>
                    <NavigationPropertyBinding Path="department_details" Target="department"/>
                </EntitySet>
                <EntitySet Name="Employes_Detail_address" EntityType="Railway.Employes_Detail_address">
                    <NavigationPropertyBinding Path="up_" Target="Employes_Detail"/>
                </EntitySet>
            </EntityContainer>
            <EntityType Name="Employes_Detail">
                <Key>
                    <PropertyRef Name="empId"/>
                </Key>
                <Property Name="firstName" Type="Edm.String"/>
                <Property Name="lastName" Type="Edm.String"/>
                <Property Name="departmentName" Type="Edm.String"/>
                <NavigationProperty Name="department_det" Type="Railway.department">
                    <ReferentialConstraint Property="departmentName" ReferencedProperty="departmentName"/>
                </NavigationProperty>
                <Property Name="empId" Type="Edm.Int32" Nullable="false"/>
                <Property Name="designation" Type="Edm.String"/>
                <NavigationProperty Name="address" Type="Collection(Railway.Employes_Detail_address)" Partner="up_">
                    <OnDelete Action="Cascade"/>
                </NavigationProperty>
            </EntityType>
            <EntityType Name="department">
                <Key>
                    <PropertyRef Name="departmentName"/>
                </Key>
                <Property Name="departmentName" Type="Edm.String" Nullable="false"/>
                <NavigationProperty Name="Emp_Id_" Type="Railway.Employes_Detail">
                    <ReferentialConstraint Property="Emp_Id" ReferencedProperty="empId"/>
                </NavigationProperty>
                <Property Name="Emp_Id" Type="Edm.Int32"/>
            </EntityType>
            <EntityType Name="organisation">
                <NavigationProperty Name="employees_details" Type="Collection(Railway.Employes_Detail)">
                    <OnDelete Action="Cascade"/>
                </NavigationProperty>
                <Property Name="employees_details_empId" Type="Edm.Int32"/>
                <NavigationProperty Name="department_details" Type="Collection(Railway.department)">
                    <OnDelete Action="Cascade"/>
                </NavigationProperty>
                <Property Name="department_details_departmentName" Type="Edm.String"/>
            </EntityType>
            <EntityType Name="Employes_Detail_address">
                <Key>
                    <PropertyRef Name="up__empId"/>
                </Key>
                <NavigationProperty Name="up_" Type="Railway.Employes_Detail" Nullable="false" Partner="address">
                    <ReferentialConstraint Property="up__empId" ReferencedProperty="empId"/>
                </NavigationProperty>
                <Property Name="up__empId" Type="Edm.Int32" Nullable="false"/>
                <Property Name="addressLine1" Type="Edm.String"/>
                <Property Name="addressLine2" Type="Edm.String"/>
                <Property Name="addressLine3" Type="Edm.String"/>
                <Property Name="state" Type="Edm.String"/>
                <Property Name="country" Type="Edm.String"/>
                <Property Name="zipCode" Type="Edm.String"/>
            </EntityType>
        </Schema>
    </edmx:DataServices>
</edmx:Edmx>